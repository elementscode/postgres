{
  "name": "@elements/postgres",
  "description": "Work easily with the postgres sql database.",
  "version": "0.0.0",
  "license": "SEE LICENSE IN LICENSE.txt",
  "browser": {
    "./connect": false,
    "./db-connection-pool": false,
    "./db-connection": false,
    "./db": false,
    "./types": false,
    "./migrate": false
  },
  "bin": {
    "migrate": "bin/migrate.js"
  },
  "engines": {},
  "dependencies": {
    "@elements/config": "0.0.0",
    "@elements/error": "0.0.0",
    "@elements/job": "0.0.0",
    "@elements/json": "0.0.0",
    "@elements/term": "0.0.0",
    "pg": ">=8.3.0 <8.4.0"
  },
  "devDependencies": {
    "@types/pg": ">=7.14.4 <8.0.0"
  },
  "scripts": {
    "clean": "rm -rf .elements/dist",
    "copyfiles": "mkdir -p .elements/dist && cp README.md LICENSE.txt package.json .elements/dist",
    "build": "yarn run clean && yarn run copyfiles && tsc",
    "build:watch": "yarn run clean && yarn run copyfiles && tsc --watch"
  }
}
